{
  "expo": {
    "name": "RuhTrack",
    "slug": "ruhtrack",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app_logo1.png",
    "scheme": "ruhtrack",
    "userInterfaceStyle": "automatic",

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "package": "com.shuaib.ruhtrack",
      "adaptiveIcon": {
        "foregroundImage": "./assets/app_logo.png",
        "backgroundColor": "#F7F5F2"
      }
    },

    "web": {
      "output": "static"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/app_logo.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ]
  }
}
