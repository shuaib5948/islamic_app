{
  "expo": {
    "name": "RuhTrack",
    "slug": "ruhtrack",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app_logo1.png",
    "scheme": "ruhtrack",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.shuaib.ruhtrack",
      "adaptiveIcon": {
        "foregroundImage": "./assets/app_logo.png",
        "backgroundColor": "#F7F5F2"
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/app_logo1.png",
      "name": "RuhTrack",
      "shortName": "RuhTrack",
      "description": "Islamic daily companion app",
      "backgroundColor": "#F7F5F2",
      "themeColor": "#90674E",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/app_logo.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "05b83551-da18-4e8b-a0a6-2e19800edbb7"
      }
    },
    "owner": "shuaib_bin_hamed"
  }
}
